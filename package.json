{
  "name": "requestinterceptmocking",
  "displayName": "requestInterceptMocking",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Request Intercept Mocking",
      "properties": {
        "requestInterceptMocking.proxyPort": {
          "type": "number",
          "default": 8000,
          "description": "Port for the proxy server"
        },
        "requestInterceptMocking.targetPort": {
          "type": "number",
          "default": 4200,
          "description": "Target port for the proxy server"
        },
        "requestInterceptMocking.automaticallyOpen": {
          "type": "boolean",
          "default": false,
          "description": "Open automatically the browser on proxy url when use mock or save request"
        }
      }
    },
    "views": {
      "requestInterceptMockingView": [
        {
          "id": "requestInterceptMockingView",
          "name": "Request Mock Data"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "requestInterceptMockingView",
          "title": "Mock available list",
          "icon": "media/mock.svg"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "extension.refreshMock",
          "group": "navigation"
        },
        {
          "command": "extension.saveRequest"
        },
        {
          "command": "extension.useMock"
        },
        {
          "command": "extension.deleteMocks"
        },
        {
          "command": "extension.stopSaveRequest"
        },
        {
          "command": "extension.stopUseMock"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.saveRequest",
        "title": "Save requests",
        "category": "Request Intercept Mocking"
      },
      {
        "command": "extension.stopSaveRequest",
        "title": "Stop save requests",
        "category": "Request Intercept Mocking"
      },
      {
        "command": "extension.showMock",
        "title": "Display mocks",
        "category": "Request Intercept Mocking"
      },
      {
        "command": "extension.useMock",
        "title": "Replace real called by mocks",
        "category": "Request Intercept Mocking"
      },
      {
        "command": "extension.stopUseMock",
        "title": "Stop replace real called by mocks",
        "category": "Request Intercept Mocking"
      },
      {
        "command": "extension.deleteMocks",
        "title": "Remove all mocks",
        "category": "Request Intercept Mocking"
      },
      {
        "command": "extension.refreshMock",
        "title": "Refresh mock list",
        "category": "Request Intercept Mocking"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/http-proxy": "^1.17.15",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.92.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "http-proxy": "^1.18.1",
    "http-proxy-middleware": "^3.0.0",
    "zlib": "^1.0.5"
  }
}
